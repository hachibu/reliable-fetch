{
    "name": "@hachibu/reliable-fetch",
    "version": "0.0.60",
    "author": {
        "name": "Raymond Sohn",
        "email": "raymondsohn@gmail.com"
    },
    "description": "An easy-to-use Node.js library to make the Fetch API more reliable.",
    "homepage": "https://hachibu.github.io/reliable-fetch",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hachibu/reliable-fetch.git"
    },
    "keywords": [
        "fetch",
        "http",
        "typescript"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.3",
        "@babel/preset-typescript": "^7.18.6",
        "@types/jest": "^29.2.1",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "babel-jest": "^29.1.2",
        "esbuild": "^0.15.13",
        "eslint": "^8.27.0",
        "husky": "^8.0.1",
        "jest": "^29.1.2",
        "jest-fetch-mock": "^3.0.3",
        "json-rules-engine": "^6.1.2",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.20",
        "typescript": "^4.8.4"
    },
    "scripts": {
        "build": "yarn run clean && yarn run build:src",
        "build:src": "yarn tsc && yarn node esbuild.mjs",
